<% unless images.empty? %>
  <div id="news-feed-images" class="images-wall">
    <% images.each do |image| %>
      <div class="col-md-6">
        <%= render "images/image", image: image %>
      </div>
    <% end %>
  </div>
  <div id="load-more-news-feed" class="load-more-news-feed">
      <%= link_to "写真をもっと見る", popular_images_path(
        image_offset: images.last.id,
        image_like_offset: images.last.like_number), remote: true %>
  </div>
<% else %>
  <h3 class="no-content"><%= t :no_content %></h3>
<% end %>
